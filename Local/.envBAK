###WEB-SERVICES
CONTAINER_WEB=web-PROYECTO-WEB
IMAGE_WEB=nginx:latest
VOLUME_DATA_WEB=./vhosts:/var/www/html/
VOLUME_CONFIG_WEB=./configs/nginx/:/etc/nginx/conf.d/
VOLUME_LOGS_WEB=./logs/:/var/log/nginx
PORT_WEB=PUERTO-WEB

###APLICATION
CONTAINER_APP=app-PROYECTO-APP
BUILD_APP=./configs/php/
VOLUME_DATA_APP=./vhosts:/var/www/html/

###DATABASE
CONTAINER_DB=db-PROYECTO-DB
IMAGE_DB=mysql:5.7
VOLUME_DATA_DB=./dbdata:/var/lib/mysql
MYSQL_DATABASE_DB=PROYECTO
MYSQL_USER_DB=wordpress
MYSQL_PASSWORD_DB=password
MYSQL_ROOT_PASSWORD=root_password
PORT_DB=PUERTO-DB